{
  "name": "morpho-data-structures",
  "version": "1.0.0",
  "description": "Data structures for Morpho's matching engine",
  "repository": "https://github.com/morpho-protocol/morpho-data-structures.git",
  "author": "Morpho Labs <security@morpho.xyz>",
  "license": "AGPL-3.0-only",
  "scripts": {
    "test": "npx hardhat test test-ts/*.ts",
    "coverage": "npx hardhat coverage",
    "lint": "yarn run lint:sol && yarn run lint:ts",
    "lint:ts": "eslint . --ext .ts",
    "lint:sol": "solhint 'contracts/**/*.sol'",
    "fix": "yarn fix:sol && yarn fix:ts",
    "fix:ts": "prettier --config .prettierrc.json './**/*.ts' --write",
    "fix:sol": "prettier --write 'src/**/*.sol'",
    "size": "npx hardhat size-contracts",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.2.3",
    "@nomiclabs/hardhat-etherscan": "2.1.8",
    "@nomiclabs/hardhat-waffle": "2.0.5",
    "@types/mocha": "10.0.1",
    "@types/node": "18.16.0",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "chai": "4.3.7",
    "dotenv": "16.0.3",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "ethers": "5.7.2",
    "hardhat": "2.14.0",
    "hardhat-contract-sizer": "2.8.0",
    "husky": "8.0.3",
    "prettier": "2.8.8",
    "prettier-plugin-solidity": "1.1.3",
    "solhint": "3.4.1",
    "solhint-plugin-prettier": "0.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  }
}
